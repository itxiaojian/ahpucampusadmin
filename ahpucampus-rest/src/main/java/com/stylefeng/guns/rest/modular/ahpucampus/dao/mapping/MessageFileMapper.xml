<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.rest.modular.ahpucampus.dao.MessageFileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.rest.modular.ahpucampus.model.MessageFile">
        <result column="id" property="id" />
        <result column="messageId" property="messageId" />
        <result column="fileName" property="fileName" />
        <result column="uuName" property="uuName" />
        <result column="url" property="url" />
        <result column="preview_url" property="previewUrl" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, messageId, fileName, uuName, url ,preview_url as previewUrl
    </sql>

</mapper>
